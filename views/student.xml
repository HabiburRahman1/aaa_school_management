<odoo>
  <data>
    <record model="ir.ui.view" id="view_student_tree">
      <field name="name">school list</field>
      <field name="model">school.student</field>
      <field name="arch" type="xml">
        <tree string="Student List">
          <field name="name"/>
          <field name="email"/>
          <field name="phone"/>
          <field name="gender"/>
          <field name="birth_date"/>
        </tree>
      </field>
    </record>


    <record model="ir.ui.view" id="view_student_form">
      <field name="name">school list</field>
      <field name="model">school.student</field>
      <field name="arch" type="xml">
        <form string="Student Form">
          <h1 class="text-center">
            Daffodil International University
          </h1>
          <field name="image" widget="image" class="oe_avatar"/>
          <group>
            <group>
              <field name="name"/>
              <field name="email"/>
              <field name="section_id"/>
            </group>
            <group>
              <field name="phone"/>
              <field name="gender"/>
              <field name="birth_date"/>
            </group>
          </group>
        </form>
      </field>
    </record>



    <record model="ir.actions.act_window" id="open_view_student_tree">
      <field name="name">Student List</field>
      <field name="res_model">school.student</field>
      <field name="view_mode">tree,form</field>
      <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
              No Record Found
          </p>
      </field>
    </record>




  </data>
</odoo>